{
   "version": "0.2.0",
   "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/ReproMtls.ConsoleApp/bin/Debug/net5.0/ReproMtls.ConsoleApp.dll",
            "args": [],
            "cwd": "${workspaceFolder}/ReproMtls.ConsoleApp",
            "console": "internalConsole",
            "stopAtEntry": false,
            "env": {
                "COMPlus_ZapDisable": "1"
            },
            "justMyCode": false,
            "symbolOptions": {
                "searchMicrosoftSymbolServer": true
            },
            "suppressJITOptimizations": true
        }
    ]
}